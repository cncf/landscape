{
  "name": "cncf-interactive-landscape",
  "version": "1.0.0",
  "description": "Interactive visualization of cloud native projects and products",
  "engines": {
    "npm": ">=3",
    "node": ">= 10.5"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "fetch": "babel-node tools/validateLandscapeKeys && babel-node tools/addExternalInfo.js && npm run yaml2json",
    "update": "(rm /tmp/landscape.json || true) && babel-node tools/validateLandscapeKeys && npm run remove-quotes && LEVEL=medium babel-node tools/addExternalInfo.js && npm run yaml2json",
    "yaml2json": "cp src/lookup.json.default src/lookup.json && babel-node tools/generateJson.js",
    "remove-quotes": "babel-node tools/removeQuotes",
    "check-links": "babel-node tools/checkLinks",
    "fetchAll": "LEVEL=complete npm run fetch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "precommit": "npm run fetch",
    "prebuild": "npm run fetch && npm run clean-dist",
    "integration-test": "(babel-node tools/ciServer &) && sleep 10 && jest",
    "render-landscape": "(babel-node tools/ciServer &) && sleep 10 && babel-node tools/renderServerless",
    "copy-dist": "cp -r _redirects src/data.json src/images src/embed.html ./dist/ && cp -r cached_logos ./dist/logos",
    "setup-robots": "babel-node tools/sitemap && babel-node tools/addRobots",
    "build": "babel-node tools/build.js && npm run copy-dist && npm run setup-robots && npm run integration-test && npm run render-landscape",
    "show-report": "open dist/report.html",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "CNCF",
  "license": "Apache-2.0",
  "dependencies": {
    "autoprefixer": "8.6.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.4",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-root-import": "6.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-minify": "0.4.3",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-webpack-plugin": "0.1.1",
    "binary-search-promises": "1.0.3",
    "bluebird": "3.5.1",
    "browser-sync": "2.24.5",
    "chalk": "2.4.1",
    "change-case": "3.0.2",
    "cheerio": "1.0.0-rc.2",
    "classnames": "^2.2.5",
    "colors": "1.3.0",
    "comma-number": "2.0.0",
    "concurrently": "3.6.0",
    "connect-history-api-fallback": "1.5.0",
    "convert-svg-to-png": "0.4.0",
    "coveralls": "3.0.1",
    "css-loader": "0.28.11",
    "csv-parse": "2.5.0",
    "csv-writer": "1.0.0",
    "current-device": "0.7.8",
    "debug": "3.1.0",
    "ejs-loader": "0.3.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-watch": "4.0.0",
    "event-emitter": "0.3.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "format-number": "3.0.0",
    "git-branch": "2.0.1",
    "history": "4.7.2",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "iframe-resizer": "3.6.1",
    "jest": "23.2.0",
    "jest-cli": "23.2.0",
    "jimp": "0.2.28",
    "js-yaml": "3.12.0",
    "jsdom": "11.11.0",
    "json-loader": "0.5.7",
    "json2csv": "4.1.5",
    "lodash-es": "4.17.10",
    "lodash-webpack-plugin": "0.11.5",
    "material-design-icons": "3.0.1",
    "material-ui": "1.0.0-beta.27",
    "millify": "2.0.1",
    "mockdate": "2.0.2",
    "node-sass": "4.9.0",
    "object-assign": "4.1.1",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.1.5",
    "prompt": "1.0.0",
    "prop-types": "15.6.2",
    "puppeteer": "1.5.0",
    "query-string": "6.1.0",
    "raf": "3.4.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-ga": "2.5.3",
    "react-hot-loader": "4.3.3",
    "react-key-handler": "1.0.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.8",
    "react-sticky-box": "0.6.2",
    "react-test-renderer": "16.4.1",
    "react-twitter-widgets": "1.7.1",
    "redux": "4.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "relative-date": "1.1.3",
    "replace": "1.0.0",
    "request": "2.87.0",
    "request-promise": "4.2.2",
    "require-promise": "1.0.1",
    "reselect": "3.0.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "sitemap": "1.13.0",
    "style-loader": "0.21.0",
    "traverse": "0.6.6",
    "twitter": "1.7.1",
    "typeface-roboto": "0.0.54",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webapp-webpack-plugin": "2.2.0",
    "webpack": "4.14.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-md5-hash": "0.0.6",
    "xml2js": "0.4.19",
    "yahoo-finance": "0.3.5"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cncf/landscape"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "rules": {
      "quotes": 0,
      "no-console": 1,
      "no-debugger": 1,
      "no-var": 1,
      "semi": [
        1,
        "always"
      ],
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-lone-blocks": 0,
      "jsx-quotes": 1,
      "react/display-name": [
        1,
        {
          "ignoreTranspilerName": false
        }
      ],
      "react/forbid-prop-types": [
        1,
        {
          "forbid": [
            "any"
          ]
        }
      ],
      "react/jsx-boolean-value": 0,
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-curly-spacing": 1,
      "react/jsx-indent-props": 0,
      "react/jsx-key": 1,
      "react/jsx-max-props-per-line": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-no-duplicate-props": 1,
      "react/jsx-no-literals": 0,
      "react/jsx-no-undef": 1,
      "react/jsx-pascal-case": 1,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/jsx-wrap-multilines": 1,
      "react/no-danger": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-direct-mutation-state": 1,
      "react/no-multi-comp": 1,
      "react/no-set-state": 0,
      "react/no-unknown-property": 1,
      "react/prefer-es6-class": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "import/extensions": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1
    },
    "globals": {}
  },
  "devDependencies": {}
}
